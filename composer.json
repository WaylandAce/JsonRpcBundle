{
    "name": "waylandace/json-rpc-bundle",
    "type": "symfony-bundle",
    "description": "JSON-RPC 2.0 Server for Symfony",
    "keywords": [
        "symfony",
        "json-rpc"
    ],
    "homepage": "https://github.com/NeoFusion/JsonRpcBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Evgeniy NeoFusion",
            "email": "evgeniy@neofusion.ru"
        }
    ],
    "require": {
        "php": "^7.2",
        "symfony/framework-bundle": "^4.2",
        "ext-json": "*",
        "phpdocumentor/reflection-docblock": "^4.3"
    },
    "require-dev": {
        "symfony/asset": "^4.2",
        "symfony/browser-kit": "^4.2",
        "symfony/phpunit-bridge": "^4.2",
        "symfony/templating": "^4.2",
        "symfony/translation": "^4.2",
        "symfony/yaml": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "NeoFusion\\JsonRpcBundle\\": ""
        }
    }
}
